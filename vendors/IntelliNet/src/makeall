#!/bin/sh -x
rm -f ../lib/*.a ../lib/*.so

rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=CCITT DEBUG_BUILD=yes
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=CCITT DEBUG_BUILD=no
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=ANSI DEBUG_BUILD=yes
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=ANSI DEBUG_BUILD=no
rm -f *.o */*.o
make PROTOCOL=ANSI SCCP_PROT=CCITT DEBUG_BUILD=yes
rm -f *.o */*.o
make PROTOCOL=ANSI SCCP_PROT=CCITT DEBUG_BUILD=no
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=TTC DEBUG_BUILD=yes
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=TTC DEBUG_BUILD=no
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=PRC DEBUG_BUILD=yes
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=PRC DEBUG_BUILD=no
rm -f *.o */*.o
make PROTOCOL=PRC SCCP_PROT=PRC DEBUG_BUILD=yes
rm -f *.o */*.o
make PROTOCOL=PRC SCCP_PROT=PRC DEBUG_BUILD=no
rm -f *.o */*.o
make PROTOCOL=TTC SCCP_PROT=TTC DEBUG_BUILD=yes
rm -f *.o */*.o
make PROTOCOL=TTC SCCP_PROT=TTC DEBUG_BUILD=no
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=CCITT DEBUG_BUILD=yes CCITT_USE_16_BIT_PC=yes
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=CCITT DEBUG_BUILD=no CCITT_USE_16_BIT_PC=yes
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=CCITT DEBUG_BUILD=yes CCITT_USE_16_BIT_PC=yes USE_CPOT_STACK=yes
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=CCITT DEBUG_BUILD=no CCITT_USE_16_BIT_PC=yes USE_CPOT_STACK=yes
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=CCITT DEBUG_BUILD=yes DUAL_TCAP=yes
rm -f *.o */*.o
make PROTOCOL=CCITT SCCP_PROT=CCITT DEBUG_BUILD=no DUAL_TCAP=yes
rm -f *.o */*.o
make PROTOCOL=ANSI SCCP_PROT=ANSI DEBUG_BUILD=yes DUAL_TCAP=yes
rm -f *.o */*.o
make PROTOCOL=ANSI SCCP_PROT=ANSI DEBUG_BUILD=no DUAL_TCAP=yes
rm -f *.o */*.o
