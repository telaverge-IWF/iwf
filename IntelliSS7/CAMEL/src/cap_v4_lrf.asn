	gmscAddress							[0] ISDN-AddressString						OPTIONAL,
	forwardingDestinationNumber			[1] CalledPartyNumber --{bound}--				OPTIONAL,
	ms-Classmark2						[2] MS-Classmark2							OPTIONAL,
