#!/bin/sh

${MAKE:-make} PROTOCOL=CCITT DEBUG_BUILD=yes clean
${MAKE:-make} PROTOCOL=CCITT DEBUG_BUILD=no clean
${MAKE:-make} PROTOCOL=CCITT DEBUG_BUILD=yes CCITT_USE_16_BIT_PC=yes USE_CPOT_STACK=yes clean
${MAKE:-make} PROTOCOL=CCITT DEBUG_BUILD=no CCITT_USE_16_BIT_PC=yes USE_CPOT_STACK=yes clean

