#!/bin/sh

${MAKE:-make} PROTOCOL=ANSI SCCPPROT=ANSI DEBUG_BUILD=yes clean
${MAKE:-make} PROTOCOL=CCITT SCCPPROT=CCITT DEBUG_BUILD=yes clean
${MAKE:-make} PROTOCOL=PRC SCCPPROT=PRC DEBUG_BUILD=yes clean
${MAKE:-make} PROTOCOL=TTC SCCPPROT=TTC DEBUG_BUILD=yes clean

${MAKE:-make} PROTOCOL=ANSI SCCPPROT=ANSI DEBUG_BUILD=no clean
${MAKE:-make} PROTOCOL=CCITT SCCPPROT=CCITT DEBUG_BUILD=no clean
${MAKE:-make} PROTOCOL=PRC SCCPPROT=PRC DEBUG_BUILD=no clean
${MAKE:-make} PROTOCOL=TTC SCCPPROT=TTC DEBUG_BUILD=no clean

${MAKE:-make} PROTOCOL=ANSI SCCPPROT=PRC DEBUG_BUILD=no clean
${MAKE:-make} PROTOCOL=ANSI SCCPPROT=PRC DEBUG_BUILD=yes clean
${MAKE:-make} PROTOCOL=ANSI SCCPPROT=CCITT DEBUG_BUILD=yes clean
${MAKE:-make} PROTOCOL=CCITT SCCPPROT=ANSI DEBUG_BUILD=yes clean
${MAKE:-make} PROTOCOL=ANSI SCCPPROT=TTC DEBUG_BUILD=yes clean
${MAKE:-make} PROTOCOL=ANSI SCCPPROT=CCITT DEBUG_BUILD=no clean
${MAKE:-make} PROTOCOL=CCITT SCCPPROT=ANSI DEBUG_BUILD=no clean
${MAKE:-make} PROTOCOL=ANSI SCCPPROT=TTC DEBUG_BUILD=no clean
${MAKE:-make} PROTOCOL=CCITT SCCPPROT=TTC DEBUG_BUILD=yes clean
${MAKE:-make} PROTOCOL=CCITT SCCPPROT=PRC DEBUG_BUILD=yes clean
${MAKE:-make} PROTOCOL=CCITT SCCPPROT=TTC DEBUG_BUILD=no clean
${MAKE:-make} PROTOCOL=CCITT SCCPPROT=PRC DEBUG_BUILD=no clean

