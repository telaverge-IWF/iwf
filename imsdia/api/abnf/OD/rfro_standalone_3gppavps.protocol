/**
 * 
   ETSI TS 132 299 V7.1.0 (2006-03) ---Rf and Ro interface based on basic Diameter protocol
   & ETSI TS 132 299 V6.6.0 (2006-03)   
   Rf interface between CTF and CDF
   Ro interface between CTF and OCF
   AVPs specific to 3GPP.
   AVPs mentioned in Rel 6 and Rel 7  are same
 */
protocol diameter.rfro.ims < 0 10415: "ETSI TS 132 299 V7.1.0 (2006-03)" >;

    //////////////////
    // Derived AVPs //
    //////////////////

/**
 * The MSISDN AVP is of type OctetString. This AVP contains an MSISDN, in international number format as described
 * in ITU-T Rec E.164, encoded as a TBCD-string, i.e. digits from 0 through 9 are encoded 0000 to 1001; 1111 is used
 * as a filler when there is an odd number of digits; bits 8 to 5 of octet n encode digit 2n; bits 4 to 1 of octet n encode digit
 * 2(n-1)+1.
 */
MSISDN                                            ::=  701 /*6.3.2*/  OctetString M,V| | |    |N;

/**
 * The Server-Name 3 is of type UTF8String. This AVP contains a SIP-URL (as defined in IETF RFC 3261 and IETF
 * RFC 2396), used to identify a SIP server (e.g. S-CSCF name).
 */
Server-Name                                       ::=  602 /*6.3.3*/  UTF8String  M,V| | |    |N;
                                                                                                                             
/**
 * The Server-Capabilities AVP (is of type Grouped. This AVP contains information to assist the I-CSCF in the selection
 * of an S-CSCF.
 */
Server-Capabilities                               ::=  603 /*6.3.4*/  Grouped     M,V| | |    |N;
                                                                                                                             
/**
 * The Mandatory-Capability AVP is of type Unsigned32. The value included in this AVP can be used to represent a
 * single determined mandatory capability of an S-CSCF. Each mandatory capability available in an individual operator¡¯s
 * network shall be allocated a unique value. The allocation of these values to individual capabilities is an operator issue. */
Mandatory-Capability                              ::=  604 /*6.3.5*/  Unsigned32  M,V| | |    |N;
                                                                                                                             
/**
 * The Optional-Capability AVP is of type Unsigned32. The value included in this AVP can be used to represent a single
 * determined optional capability of an S-CSCF. Each optional capability available in an individual operator¡¯s network
 * shall be allocated a unique value. The allocation of these values to individual capabilities is an operator issue.
 */
Optional-Capability                               ::=  605 /*6.3.6*/  Unsigned32  M,V| | |    |N;

/**
 * 3GPP-Charging-Id AVP is of type Unsigned32 and contains the Charging ID for 
 * this PDP context (this together with GGSN-Address constitutes a unique identifier
 * for the PDP context).
 */                                                                                                               
    RfRo-3GPP-Charging-Id                     ::= 2   /*Ro    */ Unsigned32   | | |     |N; /* 3GPP TS 29.061*/

/**
 * 3GPP-PDP-Type AVP is of type Unsigned32 and it denotes the type of PDP
 * context e.g IP or PPP.
 */
    RfRo-3GPP-PDP-Type                        ::= 3   /*Ro    */ Unsigned32   | | |     |N; /* 3GPP TS 29.061*/

/**
 * 3GPP-GPRS-Negotiated-QoS-Profile AVP is of type UTF8String. It is the
 * QoS Profile applied by GGSN.
 */
    RfRo-3GPP-GPRS-Negotiated-QoS-Profile      ::= 5   /*16.4.7  */ UTF8String  M,V|P| |     |Y; /* 3GPP TS 29.061 V6.7*/

/**
 * 3GPP-IMSI-MCC-MNC AVP is of type UTF8String. It contains MCC and MNC extracted from the user's
 * IMSI (first 5 or 6 digits, as applicable from the presented IMSI).
 */
    RfRo-3GPP-IMSI-MCC-MNC                     ::= 8    /*Ro      */ UTF8String   | | |     |N; /* 3GPP TS 29.061*/

/**
 * 3GPP-GGSN-MCC-MNC AVP is of type UTF8String. It contains the MCC and MNC of the 
 *  network the GGSN belongs to.
 */
    RfRo-3GPP-GGSN-MCC-MNC                     ::= 9    /*Ro      */ UTF8String   | | |     |N; /* 3GPP TS 29.061*/

/**
 * 3GPP-NSAPI AVP is of type UTF8String and it identifies a particular PDP.
 */
    RfRo-3GPP-NSAPI                            ::= 10   /*Ro      */ UTF8String   | | |     |N; /* 3GPP TS 29.061*/

/**
 * 3GPP-Session-Stop-Indicator AVP is of type Octetstring. It indicates to the AAA server
 * that the last PDP context of a session is released and that the PDP session has been terminated.
 */
    RfRo-3GPP-Session-Stop-Indicator           ::= 2001 /*Ro      */ OctetString  | | |     |N; /* 3GPP TS 29.061*/ /* Dummy AVP code '2001' defined by Kamakshi*/

/**
 * 3GPP_Selection-Mode AVP is of type UTF8String. It contains the selection mode for this
 * PDP context received in the create PDP context request message.
 */
    RfRo-3GPP-Selection-Mode                   ::= 12   /*Ro      */ UTF8String   | | |     |N; /* 3GPP TS 29.061*/

/**
 * 3GPP-Charging-Characteristics AVP is of type UTF8String. It contains the charging
 * characteristics for this PDP context received in the Create PDP Context Request 
 * Message (only available in R99 and later releases).
 */
    RfRo-3GPP-Charging-Characteristics         ::= 13   /*Ro      */ UTF8String   | | |     |N; /* 3GPP TS 29.061*/

/**
 * 3GPP-SGSN-MCC-MNC AVP is of type UTF8String. It contains the MCC and MNC extracted from 
 * the RAI within the Create PDP Context Request or Update PDP Context Request message.
 */
    RfRo-3GPP-SGSN-MCC-MNC                     ::= 18   /*Ro      */ UTF8String   | | |     |N; /* 3GPP TS 29.061*/

/**
 * Charging-Rule-Base-Name AVP is of type OctetString and it indicates the name of a 
 *  pre-defined group of charging rules residing at the TPF.
 */
    Charging-Rule-Base-Name                    ::= 1004 /* 5.2.5  */ OctetString M,V|P| |     |Y; /* 3GPP TS 29.210*/

/**
 * 3GPP-RAT-Type AVP is of type OctetString. It indicates which Radio Access Technology is 
 * currently serving the UE.
 */
    RfRo-3GPP-RAT-Type                         ::= 21   /*16.4.7  */ OctetString M,V|P| |     |Y; /* 3GPP TS 29.061 V6.7*/

/**
 * 3GPP-User-Location-Info AVP is of type Octetstring. It indicates details of where the
 *  UE is currenly located (e.g SAI or CGI).
 */
    RfRo-3GPP-User-Location-Info               ::= 22   /*16.4.7  */ OctetString M,V|P| |     |Y; /* 3GPP TS 29.061 V6.7*/

/**
 * 3GPP-MS-TimeZone AVP is of type OctetString. It indicates the offset between universal 
 * time and local time in steps of 15 minutes of where the MS currently resides.
 */
    RfRo-3GPP-MS-TimeZone                      ::= 23   /*16.4.7  */ OctetString M,V|P| |     |Y; /* 3GPP TS 29.061 V6.7*/

/**
 * 3GPP-CAMEL-Charging-Info AVP is of type OctetString. It is used to copy any CAMEL
 * information present in S-CDR(s).
 */
    RfRo-3GPP-CAMEL-Charging-Info              ::= 24   /*Ro  */ OctetString     M,V|P| |     |Y; /* 3GPP TS 29.061 V6.7*/

/**
 * PS-Furnish-Charging-Information AVP is of type Grouped. Its purpose is to add online charging session
 * specific information, received via the Ro Reference point onto the Rf reference point in order to 
 * facilitate its inclusion in CDRs.This information element may be received in CCA message via the Ro
 * reference point. In situations where online and offline charging are active in parallel, the information
 * element is transparently copied into ACR to be sent on the Rf reference point.
 */
    PS-Furnish-Charging-Information            ::= 865  /*7.2.86  */ Grouped     V,M|P| |     |N; /* 3GPP TS 32.299 V7.1*/

/**
 * PS-Free-Format-Data AVP is of type OctetString and holds online charging session specific data.
 */
    PS-Free-Format-Data                        ::= 866  /*7.2.85  */ OctetString V,M|P| |     |N; /* 3GPP TS 32.299 V7.1*/

/**
 * PS-Append-Free-Format-Data AVP is of type enumerated and indicates if the information sent in the
 * PS-Free-Format-Data AVP must be appended to the PS-Free-Format-Data stored for the online-session.
 */
    PS-Append-Free-Format-Data                 ::= 867  /*7.2.84  */ Enumerated  V,M|P| |     |N; /* 3GPP TS 32.299 V7.1*/

/**
 * CG-Address AVP is of type Address and holds the IP-address of the charging gateway.
 */
    CG-Address                                 ::= 846  /*7.2.19  */ Address     V,M|P| |     |Y;/* 3GPP TS 32.299 V7.1*/

/**
 * GGSN-Address AVP is of type Address and holds the IP-Address of the GGSN that generated 
 * the GPRS Charging ID.
 */
    GGSN-Address                               ::= 847  /*7.2.35  */ Address     V,M|P| |     |N;/* 3GPP TS 32.299 V7.1*/

/**
 * SGSN-Address AVP is of type Address and holds the IP-Address fo the SGSN that was
 * used during a report.
 */
    SGSN-Address                               ::= 1228 /*7.2.103 */ Address     V,M|P| |     |N;/* 3GPP TS 32.299 V7.1*/

/**
 * PDP-Address AVP is of type Address and holds the IP-Address associated with the PDP session.
 */
    PDP-Address                                ::= 1227 /*7.2.25  */ Address     V,M|P| |     |Y;/* 3GPP TS 32.299 V7.1*/

/**
 * Service-Information AVP is of type Grouped. Its purpose is to allow the transmission of
 * additional 3GPP service specific information elements which are not described in the 32.299V7.1
 */
    Service-Information                        ::= 873  /*7.2.101 */ Grouped     V,M|P| |     |N; /* 3GPP TS 32.299 V7.1*/

/**
 * PS-Information AVP is of type Grouped. Its purpose is to allow the transmission of
 * additional PS service specific information elements.
   
 */
    PS-Information                             ::= 874  /*7.2.87  */ Grouped     V,M|P| |     |N;
                                                                                                                             
/**
 * Time-Quota-Threshold AVP is of type Unsigned32 and contains a threshold value in seconds.
 */
    Time-Quota-Threshold                       ::= 868  /* 7.2.110*/ Unsigned32  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Volume-Quota-Threshold AVP is of type Unsigned32 and contains a threshold value in octets.
 */
    Volume-Quota-Threshold                     ::= 869  /* 7.2.118*/ Unsigned32  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Unit-Quota-Threshold AVP is of type Unsigned32 and contains a threshold value in service
 * specific units.
 */
    Unit-Quota-Threshold                       ::= 1226 /* 7.2.116*/ Unsigned32  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Trigger-Type AVP is of type Enumerated and indicates a single re-authorisation event type.
 * When included in the Credit Control Answer command, this AVP indicates the events that
 * shall cause the credit control client to re-authorise the associated quota. The client 
 * shall not re-authorise the quota when events which are not included in the Trigger-Type AVP occur.
 * When included in the Credit Control Request command indicates the specific event which caused the 
 * re-authorisation request of the Reporting-Reason with value RATING_CONDITION_CHANGE associated.
 */
    Trigger-Type                               ::= 870  /* 7.2.113*/ Enumerated  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Quota-Holding-Time AVP is of type Unsigned32 and contains the quots holding time in seconds.
 */
    Quota-Holding-Time                         ::= 871  /* 7.2.89 */ Unsigned32  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Reporting-Reason AVP is of type Enumerated and specifies the reason for usage reporting for
 * one or more types of quota for a particular category.
 */
    Reporting-Reason                           ::= 872  /* 7.2.93 */ Enumerated  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Quota-consumption-Time is of type Unsigned32 and contains an idle traffic threshold time in
 * seconds.
 */
    Quota-Consumption-Time                     ::= 881  /* 7.2.88 */ Unsigned32  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * WLAN-Information AVP is of type Grouped. Its purpose is to allow the transmission of additional
 * WLAN service specific information elements. The format and the contents of the fields inside the
 * WLAN-Information AVP is specified in TS 32.252.
 */
    WLAN-Information                           ::= 875  /* 7.2.121*/ Grouped     V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * WLAN-Session-Id AVP is of type Unsigned32 and contains the charging id generated by the AAA
 * server for the session.
 */
    WLAN-Session-Id                            ::= 2000 /* 7.2.123*/ Unsigned32  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/ /* Dummy AVP code '2000' defined by Kamakshi. Type defined as UTF8String in Flag rule in Table 7.2*/

/**
 * PDG-Address AVP is of type Address and contains the PDG IP address.
 */
    PDG-Address                                ::= 895  /* 7.2.73 */ Address     V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * PDG-Charging-Id is of type Unsigned32 nd contains the charging identifier generated by the PDG
 * for the tunnel. Charging identifier is generated at tunnel establishment and transferred to
 * 3GPP AAA server.
 */
    PDG-Charging-Id                            ::= 896  /* 7.2.74 */ Unsigned32  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * WAG-Address AVP is of type Address and contains the WAG IP address.
 */
    WAG-Address                                ::= 890  /* 7.2.119*/ Address     V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * WAG-PLMN-Id is of type OctetString and contains the WAG PLMN Id( MCC and MNC). Coding of
 * this AVP is same as 3GPP-MCC-MNC coding described in 3GPP TS 29.061.
 */
    WAG-PLMN-Id                                ::= 891  /* 7.2.120*/ OctetString V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * WLAN-Radio-Container AVP is of type Grouped. 
 */
    WLAN-Radio-Container                       ::= 892  /* 7.2.122*/ Grouped     V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * WLAN-Technology is of type Unsigned32.
 */
    WLAN-Technology                            ::= 893  /* 7.2.124*/ Unsigned32  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * WLAN-UE-Local-IPAddress AVP is of type Unsigned32.
 */
    WLAN-UE-Local-IPAddress                    ::= 894  /* 7.2.125*/ Address     V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * IMS-Information AVP is of type Grouped. Its purpose is to allow teh transmission of 
 * additional IMS serrvice specific information elements.
 */
    IMS-Information                            ::= 876  /* 7.2.37 */ Grouped     V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Event-Type AVP is of type grouped and containes information about the type of chargeable
 * telecommunication service/event for which the accounting-request message is generated.
 */
    Event-Type                                 ::= 823  /* 7.2.32 */ Grouped     V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * SIP-Method AVP is of type UTF8String and holds the name of the SIP Method (INVITE,UPDATE,etc)
 * causing an accounting request to be sent to the CDF.
 */
    SIP-Method                                 ::= 824  /* 7.2.104*/ UTF8String  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * The Event AVP is of type UTF8String and holds the content of the "Event" header.
 */
    IMS-Event                                  ::= 825  /* 7.2.31 */ UTF8String  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Expires AVP is of type Unsigned32 and holds the content of the "Expires" header.
 */
    Expires                                    ::= 888  /* 7.2.33 */ Unsigned32  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Role-Of-Node AVP is of type enumerated and specifies the role of the AS/CSCF.
 */
    Role-Of-Node                               ::= 829  /* 7.2.94 */ Enumerated  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Node-Functionality AVP is of type Enumerated and includes the functionality identifier of the node.
 */
    Node-Functionality                         ::= 862  /* 7.2.66 */ Enumerated  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * User-Session-ID AVP is of type UTF8String and holds the session identifier. For a SIP session the 
 * User-Session-ID contains the SIP Call ID
 */
    User-Session-ID                            ::= 830  /* 7.2.117*/ UTF8String  V,M|P| |     |N; /*3GPP TS 32.299 V7.1*/

/**
 * Calling-Party-Address is of type UTF8String and holds the address (Public User ID,SIP URL,E.164 etc.)
 * of the party initiating a session.In IMS charging the address is obtained from P-Asserted-Identity header.
 */
    Calling-Party-Address                      ::= 831  /*7.2.17  */ UTF8String  V,M|P| |     |N; /* 3GPP TS 32.299 V7.1*/

/**
 * Called-Party-Address is of type UTF8String and holds the address (Public User ID,SIP URL,E.164 etc.)
 * of the party to whom a session is established.
 */
    Called-Party-Address                       ::= 832  /*7.2.16  */ UTF8String  V,M|P| |     |N; /* 3GPP TS 32.299 V7.1*/

/**
 * Time-Stamps AVP is of type Grouped and holds the time of the initial SIP request and the time
