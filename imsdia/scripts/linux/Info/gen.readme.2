#!/bin/bash

echo "The following info was collected using UNIX tools (hostname, arch, etc)" >> README
echo "----------------------------------------------------------------------" >> README
echo "" >> README
echo "Date: `date '+%Y.%m.%d'`" >> README
echo "" >> README
echo "System Type:" >> README
head -n 1 /etc/issue >> README
echo "" >> README
echo "Uptime: `uptime`" >> README
echo "" >> README
echo "Free:" >> README
echo "`free`" >> README
echo "" >> README
echo "Vmstat:" >> README
echo "`vmstat`" >> README
echo "" >> README
echo "" >> README

# Describe dir layout information for collected data
cat ../Info/text >> README
